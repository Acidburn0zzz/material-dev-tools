{
  "name": "Material DevTools Theme",
  "version": "1.0",
  "description": "A developer tools color theme inspired by the Material Theme UI plugin for IntelliJ IDEA.",
  "manifest_version": 2,
  "author": "Elior Boukhobza (mallowigi)",
  "devtools_page": "devtools.html",
  "permissions": [
    "declarativeContent",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Get Material",
    "default_popup": "/dist/export/index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
